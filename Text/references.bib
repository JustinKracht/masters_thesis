	pages = {523--549},
	publisher = {[American Educational Research Association, Sage Publications, Inc., American Statistical Association]},
	title = {A Comparison of Maximum Likelihood and {B}ayesian Estimation for Polychoric Correlation Using Monte Carlo Simulation},
